{
  email {$ACME_EMAIL}
}

{$DOMAIN} {
  encode zstd gzip

  reverse_proxy relayserver:5104

  header {
    # basic security headers
    Strict-Transport-Security "max-age=31536000; includeSubDomains; preload"
    X-Content-Type-Options "nosniff"
    X-Frame-Options "DENY"
    Referrer-Policy "no-referrer"
  }
}


